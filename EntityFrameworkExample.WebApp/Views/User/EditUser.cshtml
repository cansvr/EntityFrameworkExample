@model EntityFrameworkExample.Data.Entities.Users
@{
    ViewBag.Title = "EditUser";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h2>Kullanıcı Düzenleme</h2>
<br />
@using (Html.BeginForm("EditUser", "User", FormMethod.Post))
{
    <div class="form-group">
        @Html.Label("Kullanıcı Kodu")
        @Html.TextBoxFor(x => x.USER_CODE, new { @class = "form-control" })
        <br />
        @Html.Label("Kullanıcı Adı")
        @Html.TextBoxFor(x => x.USER_NAME, new { @class = "form-control" })
        @Html.ValidationMessageFor(x => x.USER_NAME)
        <br />
        <button class="btn btn-info">Değişiklikleri Uygula</button>
    </div>
}


